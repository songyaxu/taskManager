<script type="text/javascript">
	function clear(){
		$("#message").text("");
	}
    $("#reset").click(function(){
	    clear();
	});
	$("#no").focus(function(){
		clear();
	})
	$("#pwd").focus(function(){
		clear();
	})
    $("#submit").click(function(){
    	if($("#no").val().length==0)
    	{
    		layer.msg('请输入用户名！', {icon: 2});
    		return false;
    	}
    	if($("#pwd").val().length==0)
    	{
    		layer.msg('请输入密码！', {icon: 2});
    		return false;
    	}
    });
    
</script>
    

    

